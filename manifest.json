{
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },

  "browser_action": {
    "default_icon": "images/worldwide.png",
    "default_title": "Quora Full Access"
  },

  "description": "An extension to access the unblock full pages on Quora and remove the nagging login/sign-up blocker..",
  "icons": {"16": "images/worldwide.png"},
  "manifest_version" : 2,
  "name": "Quora Full Access",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.quora.com/"
  ],

  "update_url":"http://clients2.google.com/service/update2/crx",
  "version": "1",

  "web_accessible_resources": [
    "images/worldwide.png"
  ]
}
